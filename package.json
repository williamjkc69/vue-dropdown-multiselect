{
  "name": "vue-dropdown-multiselect",
  "description": "Vue component to use it like a dropdown",
  "version": "0.1.3",
  "private": false,
  "main": "dist/vue-dropdown-multiselect.umd.js",
  "module": "dist/vue-dropdown-multiselect.esm.js",
  "unpkg": "dist/vue-dropdown-multiselect.min.js",
  "browser": {
    "./sfc": "src/vue-dropdown-multiselect.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-dropdown-multiselect.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-dropdown-multiselect.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-dropdown-multiselect.min.js",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "vue",
    "vue select",
    "vue multiselect",
    "vue dropdown",
    "vue component",
    "select searchable",
    "custom select",
    "custom dropdown"
  ],
  "author": {
    "name": "William Urbina",
    "email": "williamjkc69@gmail.com",
    "url": "https://williamjkc69.github.io/"
  },
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "postcss": "^8.3.0",
    "rollup": "^2.26.8",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-import-css": "^2.0.1",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-vue": "^5.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
